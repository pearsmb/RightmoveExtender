{
  "manifest_version": 3,
  "name": "Rightmove Extender",
  "version": "0.1",
  "description": "Searches for the nearest locations of users choosing.",
  "content_scripts": [{
    "matches": [
        "*://*.rightmove.co.uk/*",
        "*://*.google.com/*"
    ],
    "css": [
        "main.css"
    ],
    "js": ["main.js", "rightmoveClassNames.js"]
  }],
  "host_permissions": ["<all_urls>"],
  "permissions": ["storage"],
  "options_ui": {
    "page": "opt.html"
  }
}